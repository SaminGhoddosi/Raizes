  CONSTRAINT `fk_pi_Insumo_EquipamentoPlantio`
    FOREIGN KEY (`EquipamentoId`)
    REFERENCES `Raizes`.`Equipamento` (`Id`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
